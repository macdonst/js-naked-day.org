{
  "name": "enhance-app",
  "version": "0.1.13",
  "scripts": {
    "start": "npx enhance dev",
    "lint": "eslint ./app/**/*.mjs --fix",
    "enhance": "enhance",
    "test": "npm run lint"
  },
  "devDependencies": {
    "@architect/architect": "^11.1.0",
    "@enhance/cli": "latest",
    "@enhance/types": "^0.7.0",
    "eslint": "^8.49.0"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "extends": "eslint:recommended",
    "rules": {
      "indent": [
        "error",
        2
      ]
    },
    "ignorePatterns": [],
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2022
    }
  },
  "dependencies": {
    "@enhance/arc-plugin-enhance": "^11.0.4",
    "@enhance/arc-plugin-styles": "^5.0.6",
    "@enhance/styles-cribsheet": "^0.0.11"
  }
}
